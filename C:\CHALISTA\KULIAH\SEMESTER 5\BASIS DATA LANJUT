          ]
        }
      ],
      "source": [
        "# A simple example.\n",
        "q = cirq.GridQubit(5, 2)\n",
        "circuit = cirq.Circuit(cirq.X(q) ** 0.5, cirq.measure(q, key='m'))\n",
        "\n",
        "job = processor.run_sweep(program=circuit, repetitions=1000)\n",
        "\n",
        "results = [str(int(b)) for b in job.results()[0].measurements['m'][:, 0]]\n",
        "print('Success!  Results:')\n",
        "print(''.join(results))"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "data_karyawan = {}\n",
        "\n",
        "def Tambah_Data():\n",
        "    nm = input(\"Nama\\t\\t: \")\n",
        "    us = int(input(\"Usia\\t\\t: \"))\n",
        "    jbt = input(\"Jabatan\\t\\t: \")\n",
        "    dep = input(\"Departement\\t: \")\n",
        "\n",
        "    id = len(data_karyawan) + 1\n",
        "\n",
        "    data_karyawan[id] = {\n",
        "        'nama': nm,\n",
        "        'usia': us,\n",
        "        'jabatan': jbt,\n",
        "        'departement': dep\n",
        "    }\n",
        "    print(f\"\\nData karyawan {nm} sudah ditambahkan dengan ID: {id}\")\n",
        "\n",
        "print(data_karyawan)\n",
        "def Hapus_Data(id):\n",
        "    if id in data_karyawan:\n",
        "        del data_karyawan[id]\n",
        "        print(f\"Karyawan dengan ID {id} sudah dihapus.\")\n",
        "    else:\n",
        "        print(f\"Data karyawan dengan ID {id} tidak ditemukan.\")\n",
        "def Info_Karyawan(id):\n",
        "    karyawan = data_karyawan.get(id)\n",
        "    if karyawan:\n",
        "        print(f\"ID Karyawan: {id}\")\n",
        "        print(f\"Name: {karyawan['nama']}\")\n",
        "        print(f\"Usia: {karyawan['usia']}\")\n",
        "        print(f\"Jabatan: {karyawan['jabatan']}\")\n",
        "    else:\n",
        "        print(f\"Data karyawan dengan ID {id} tidak ditemukan.\")\n",
        "def Tampilkan_Data():\n",
        "    print(\"Data Karyawan:\")\n",
        "    for id, nama in data_karyawan.items():\n",
        "        print(f\"ID: {id}, Data: {data_karyawan[id]}\")"
      ],
