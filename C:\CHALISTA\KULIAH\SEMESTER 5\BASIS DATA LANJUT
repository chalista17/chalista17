        "while(True):\n",
        "  print(\"\\n --- Data Karyawan ---\")\n",
        "  print(\"1. Lihat Data\")\n",
        "  print(\"2. Tambah Data\")\n",
        "  print(\"3. Ubah Data\")\n",
        "  print(\"4. Hapus\")\n",
        "  print(\"5. Selesai\")\n",
        "  pilih = input(\"Pilih Menu (1-5)\")\n",
        "\n",
        "  if(pilih == '1'):\n",
        "    Tampilkan_Data()\n",
        "  elif(pilih == '2'):\n",
        "    Tambah_Data()\n",
        "  elif(pilih == '3'):\n",
        "    Update_Data()\n",
        "  elif(pilih == '4'):\n",
        "    Hapus_Data()\n",
        "  elif(pilih == '5'):\n",
        "    print(\"Terimakasih\")\n",
        "  break"
      ],
