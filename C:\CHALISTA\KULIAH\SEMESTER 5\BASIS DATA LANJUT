        "def Update_Data():\n",
        "    id = int(input(\"Masukkan ID karyawan yang ingin diupdate: \"))\n",
        "\n",
        "    if id in data_karyawan:\n",
        "        print(\"\\nData lama:\")\n",
        "        print(data_karyawan[id])\n",
        "\n",
        "        print(\"\\nMasukkan data baru (kosongkan jika tidak ingin diubah)\")\n",
        "\n",
        "        nm = input(\"Nama baru\\t\\t: \")\n",
        "        us = input(\"Usia baru\\t\\t: \")\n",
        "        jbt = input(\"Jabatan baru\\t\\t: \")\n",
        "        dep = input(\"Departement baru\\t: \")\n",
        "\n",
        "        if nm:\n",
        "            data_karyawan[id]['nama'] = nm\n",
        "        if us:\n",
        "            data_karyawan[id]['usia'] = int(us)\n",
        "        if jbt:\n",
        "            data_karyawan[id]['jabatan'] = jbt\n",
        "        if dep:\n",
        "            data_karyawan[id]['departement'] = dep\n",
        "\n",
        "        print(\"\\nData karyawan berhasil diperbarui.\\n\")\n",
        "    else:\n",
        "        print(\"Data karyawan dengan ID tersebut tidak ditemukan.\\n\")"
      ],
      "metadata": {
        "id": "a-gUjjZqVp9m"
      },
      "execution_count": 32,
      "outputs": []
    },
